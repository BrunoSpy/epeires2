{
    "name": "dsna/epeires2",
    "description": "Cahier de marche \u00e9lectronique Exploitation",
    "license": "AGPL-3.0-or-later",
    "homepage": "http://asap.dsna.fr/projects/pvcds",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BrunoSpy/opentbsbundle"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "lib-curl": "*",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.1",
        "laminas/laminas-mvc-form": "^2.0",
        "laminas/laminas-log": "^2.10",
        "laminas/laminas-mail": "^2.12",
        "laminas/laminas-code": "^4.5.0",
        "laminas/laminas-file": "^2.8",
        "laminas/laminas-progressbar": "^2.7",
        "laminas/laminas-dependency-plugin": "^2.0",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-mvc-i18n": "^1.1",
        "laminas/laminas-cli": "^1.0",
        "laminas/laminas-router": "^3.7",
        "laminas/laminas-development-mode": "^3.2",
        "laminas/laminas-component-installer": "^3.0",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "laminas/laminas-form": "^3.9",
        "doctrine/doctrine-orm-module": "^5.0",
        "roave/psr-container-doctrine": "^3.0.0",
        "gedmo/doctrine-extensions": "^3.0",
        "lm-commons/lmc-user": "^3.3.2",
        "lm-commons/lmc-user-doctrine-orm": "^3.0",
        "lm-commons/lmc-rbac-mvc": "^3.0",,
        "lm-commons/lmc-cors": "^1.2",
        "netiul/dompdf-module": "^0.7.0",
        "kipperlenny/opentbs-bundle": "dev-master",
        "rlanvin/php-rrule": "^2.2",
        "maglnet/magl-markdown": "~1.1",
        "dgac/nmb2b-phpclient": "^0.5.0",
        "dgac/mattermostmodule": "^0.4.0",
        "laminas-api-tools/api-tools": "^1.4",
        "laminas-api-tools/api-tools-documentation": "^1.3",
        "laminas-api-tools/api-tools-documentation-swagger": "^1.3",
        "beberlei/doctrineextensions": "^1.2",
        "linkorb/jsmin-php": "^1.0",
        "php-defer/php-defer": "^4.0|^5.0",
        "samsonasik/force-https-module": "^4.0",
        "rwoverdijk/assetmanager": "^3.0",
        "doctrine/dbal": "^3.2"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "api-skeletons/doctrine-data-fixture": "^6.0.7",
        "laminas-api-tools/api-tools-admin": "^2.0",
        "laminas-api-tools/api-tools-asset-manager": "^1.2",
        "codeception/module-webdriver": "^3.0.0",
        "codeception/module-asserts": "^3.0.0",
        "codeception/module-laminas": "^3.0.0",
        "codeception/module-doctrine2": "^3.0.0",
        "laminas/laminas-developer-tools": "^2.0"
    },
    "replace": {
        "laminas/laminas-cache-storage-adapter-apc": "*",
        "laminas/laminas-cache-storage-adapter-dba": "*",
        "laminas/laminas-cache-storage-adapter-memcache": "*",
        "laminas/laminas-cache-storage-adapter-memcached": "*",
        "laminas/laminas-cache-storage-adapter-mongodb": "*",
        "laminas/laminas-cache-storage-adapter-wincache": "*",
        "laminas/laminas-cache-storage-adapter-xcache": "*",
        "laminas/laminas-cache-storage-adapter-zend-server": "*"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "laminas/laminas-dependency-plugin": true,
            "mindplay/composer-locator": true,
            "laminas-api-tools/api-tools-asset-manager": true,
            "laminas/laminas-component-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Core\\": "module/Core/src/",
            "Application\\": "module/Application/src/",
            "IPO\\": "module/IPO/src/",
            "Administration\\": "module/Administration/src/",
            "API\\": "module/API/src/"
        }
    },
    "scripts": {
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public"
    },
    "scripts-descriptions": {
        "development-disable": "Disable development mode.",
        "development-enable": "Enable development mode.",
        "development-status": "Detail whether or not the application is in development mode.",
        "serve": "Start the built-in PHP web server and serve the application."
    }
}
